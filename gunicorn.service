[Unit]
Description=demo fastapi application
After=network.target

[Service]
User=linush
Group=linush
WorkingDirectory=/home/linush/app/src/
Environment="PATH=/home/linush/app/venv/bin"
EnvironmentFile=/home/linush/.env
ExecStart=/home/linush/app/venv/bin/gunicorn -w 4 -k uvicorn.workers.UvicornWorker app.main:app --bind 0.0.0.0:8000

[Install]
WantedBy=multi-user.target